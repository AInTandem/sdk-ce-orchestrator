{
  "name": "@aintandem/sdk-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript SDK for AInTandem CE Orchestrator",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @aintandem/sdk-core dev",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test:coverage",
    "test:unit": "pnpm -r test:unit",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "generate-types": "tsx scripts/generate-types.ts",
    "sync-types": "tsx scripts/sync-types.ts",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "docs:generate": "typedoc",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "keywords": [
    "typescript",
    "sdk",
    "api-client",
    "aintandem"
  ],
  "author": "AInTandem Team",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "openapi-typescript-codegen": "^0.30.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^4.0.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.14.0"
}
