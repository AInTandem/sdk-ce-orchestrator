{
  "name": "@aintandem/sdk-monorepo",
  "version": "0.5.2",
  "private": true,
  "description": "TypeScript SDK for AInTandem CE Orchestrator",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @aintandem/sdk-core dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:coverage": "vitest run --config vitest.e2e.config.ts --coverage",
    "test:all": "pnpm test && pnpm test:e2e",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "generate-types": "tsx scripts/generate-types.ts",
    "sync-types": "tsx scripts/sync-types.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "pnpm build && changeset publish",
    "docs:generate": "typedoc",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "hooks:install": "sh scripts/setup-hooks.sh",
    "deps:workspace": "tsx scripts/set-deps-mode.ts workspace && pnpm install",
    "deps:npm": "tsx scripts/set-deps-mode.ts npm && pnpm install",
    "dev:setup": "tsx scripts/set-deps-mode.ts workspace && pnpm install && pnpm hooks:install"
  },
  "keywords": [
    "typescript",
    "sdk",
    "api-client",
    "aintandem"
  ],
  "author": "AInTandem Team",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "eslint": "^8.56.0",
    "openapi-typescript": "^7.10.1",
    "openapi-typescript-codegen": "^0.30.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.27.0"
}
