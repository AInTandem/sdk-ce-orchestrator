{
  "name": "@aintandem/sdk-core",
  "version": "0.5.1",
  "description": "Core TypeScript SDK for AInTandem CE Orchestrator API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "sdk",
    "api-client",
    "aintandem"
  ],
  "author": "AInTandem Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AInTandem/sdk-ce-orchestrator"
  },
  "bugs": {
    "url": "https://github.com/AInTandem/sdk-ce-orchestrator/issues"
  },
  "homepage": "https://github.com/AInTandem/sdk-ce-orchestrator#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.2.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {},
  "peerDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
